generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model account {
  accountNo  Int   @id
  currentBal Float
}

model transactions {
  transactionId   String   @id
  transactionType String
  amount          Float
  oldBalance      Float
  newBalance      Float
  status          String
  createdAt       DateTime @default(now())
  updatedAt       DateTime
  flag            String?
  alertType       String?
  details         String?
  accountNo       Int
}
